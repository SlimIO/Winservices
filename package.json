{
  "name": "@slimio/winservices",
  "version": "1.2.0",
  "description": "Windows Services - Node.JS low level binding",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "prebuilds": "prebuildify --napi",
    "build": "cross-env node-gyp configure && node-gyp build",
    "test": "cross-env psp && ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html",
    "install": "node-gyp rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Winservices.git"
  },
  "keywords": [
    "SlimIO",
    "Monitoring",
    "Windows",
    "Services",
    "Binding",
    "NAPI",
    "Microsoft"
  ],
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "files": [
    "index.js",
    "index.d.ts",
    "winservices.cpp",
    "binding.gyp",
    "include/*.h"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/SlimIO/Winservices/issues"
  },
  "homepage": "https://github.com/SlimIO/Winservices#readme",
  "dependencies": {
    "node-addon-api": "^1.7.1",
    "node-gyp-build": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^3.0.3",
    "@slimio/is": "^1.5.1",
    "@slimio/psp": "^0.9.2",
    "ava": "^2.4.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "husky": "^3.0.4",
    "node-gyp": "^5.0.3",
    "nyc": "^14.1.1",
    "pkg-ok": "^2.3.1",
    "prebuildify": "^3.0.4"
  }
}
